terminal           PLUS, MINUS, TIMES, DIVIDE, MOD;
terminal           LPAREN, RPAREN;
terminal Integer   NUMBER;

nonterminal Integer expr;

expr ::= expr PLUS   expr
      |  expr MINUS  expr
      |  expr TIMES  expr
      |  expr DIVIDE expr
      |  expr MOD    expr
      |  NUMBER               
      |  MINUS expr
      |  LPAREN expr RPAREN   
      ;
  


  

